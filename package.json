{
  "name": "@beanstalk/sdk",
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "npx --node-options=\"--no-warnings\" hardhat test",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "generate": "yarn generate:typechain && yarn generate:graphql",
    "generate:typechain": "typechain --target=ethers-v5 --ts-nocheck --out-dir=./src/constants/generated/ './src/constants/abi/**/*.json'",
    "generate:graphql": "graphql-codegen --config codegen.ts",
    "copyabis": "rm -rf ./src/constants/abi && cp -R ../Beanstalk-UI/src/constants/abi ./src/constants/abi"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 140,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "module": "dist/sdk.esm.js",
  "peerDependencies": {
    "ethers": ">=5.6.8"
  },
  "devDependencies": {
    "@0x/abi-gen": "^5.8.1",
    "@foundry-rs/hardhat-anvil": "^0.1.6",
    "@graphql-codegen/cli": "2.13.7",
    "@graphql-codegen/typescript": "2.7.4",
    "@graphql-codegen/typescript-graphql-request": "^4.5.6",
    "@graphql-codegen/typescript-operations": "^2.5.4",
    "@graphql-codegen/typescript-resolvers": "2.7.4",
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@typechain/ethers-v5": "^10.1.0",
    "@types/jest": "^29.1.2",
    "@types/lodash": "^4.14.186",
    "@types/lodash-es": "^4",
    "chai": "^4.3.6",
    "dotenv": "^16.0.3",
    "ethers": ">=5.6.8",
    "hardhat": "^2.11.2",
    "husky": "^8.0.1",
    "jest": "^29.2.0",
    "mocha": "^10.0.0",
    "ts-jest": "^29.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typechain": "^8.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "bignumber.js": "^9.1.0",
    "graphql": "^16.6.0",
    "graphql-request": "^4",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21"
  },
  "resolutions": {
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.2.3"
}
