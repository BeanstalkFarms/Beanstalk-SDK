{
  "name": "@beanstalk/utils",
  "version": "0.1.0-beta.8",
  "description": "Beanstalk protocol utilities for testing and logging",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BeanstalkFarms/Beanstalk-SDK.git",
    "directory": "packages/utils"
  },
  "homepage": "https://bean.money",
  "keywords": [
    "stablecoin",
    "eth",
    "ethereum",
    "dapps",
    "web3"
  ],
  "main": "./dist/sdk/sdk.cjs.js",
  "module": "./dist/sdk/sdk.esm.js",
  "browser": "./dist/sdk/sdk.udm.js",
  "typings": "./dist/types/index.d.ts",
  "licenses": [],
  "files": [
    "/dist",
    "README.md"
  ],
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "dev": "tsc --noEmit --watch",
    "ts": "rimraf dist && tsc && tsc-alias -p tsconfig.json",
    "build": "yarn ts && rollup -c --bundleConfigAsCjs --silent && rimraf dist/js",
    "publish": "yarn build && yarn npm publish --access public"
  },
  "peerDependencies": {
    "ethers": "5.6.8"
  },
  "dependencies": {
    "@ethersproject/bytes": "5.7.0",
    "@ethersproject/logger": "5.7.0"
  },
  "devDependencies": {
    "@foundry-rs/hardhat-anvil": "^0.1.6",
    "@nomiclabs/hardhat-ethers": "^2.1.1",
    "@rollup/plugin-commonjs": "23.0.2",
    "@rollup/plugin-json": "5.0.1",
    "@rollup/plugin-multi-entry": "6.0.0",
    "@rollup/plugin-node-resolve": "15.0.1",
    "@types/eslint": "^8",
    "@types/jest": "^29.1.2",
    "@types/rimraf": "^3",
    "chai": "4.3.6",
    "dotenv": "^16.0.3",
    "eslint": "8.26.0",
    "jest": "^29.2.0",
    "rimraf": "3.0.2",
    "rollup": "3.2.5",
    "rollup-plugin-alias": "2.2.0",
    "rollup-plugin-exclude-dependencies-from-bundle": "1.1.23",
    "rollup-plugin-typescript2": "0.34.1",
    "ts-jest": "^29.0.3",
    "ts-node": "10.9.1",
    "tsc-alias": "1.7.0",
    "tslib": "2.4.1",
    "typescript": "4.8.4"
  },
  "packageManager": "yarn@3.2.3",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "module": "./dist/sdk/sdk.esm.js",
      "default": "./dist/sdk/sdk.cjs.js",
      "browser": "./dist/sdk/sdk.umd.js"
    },
    "./DecimalBigNumber": {
      "types": "./dist/types/DecimalBigNumber.d.ts",
      "module": "./dist/DecimalBigNumber/DecimalBigNumber.esm.js",
      "default": "./dist/DecimalBigNumber/DecimalBigNumber.cjs.js",
      "browser": "./dist/DecimalBigNumber/DecimalBigNumber.umd.js"
    },
    "./TokenValue": {
      "types": "./dist/types/TokenValue.d.ts",
      "module": "./dist/TokenValue/TokenValue.esm.js",
      "default": "./dist/TokenValue/TokenValue.cjs.js",
      "browser": "./dist/TokenValue/TokenValue.umd.js"
    }
  }
}
